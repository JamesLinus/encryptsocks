{
  "name": "shadowsocks-js",
  "version": "1.1.0",
  "description": "Yet another shadowsocks implementation for nodejs to help you bypass firewalls.",
  "keywords": [
    "shadowsocks",
    "ss",
    "SOCKS5",
    "firewall"
  ],
  "bin": {
    "serverssjs": "./bin/serverssjs",
    "localssjs": "./bin/localssjs"
  },
  "scripts": {
    "build": "babel src --out-dir lib && eslint src",
    "watch": "babel src --watch --out-dir lib",
    "test": "mocha",
    "lint": "eslint src",
    "travis-ci-test": "mocha --fgrep '[local only]' --invert"
  },
  "author": "oyyd (oyydoibh@gmail.com)",
  "license": "BSD",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "eslint": "^2.7.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.3.0",
    "mocha": "^2.4.5",
    "socks": "^1.1.9",
    "socks5-http-client": "^1.0.2",
    "socks5-https-client": "^1.1.2"
  },
  "dependencies": {
    "ip": "^1.1.3",
    "lru-cache": "^4.0.1",
    "minimist": "^1.2.0",
    "winston": "^2.2.0"
  }
}
